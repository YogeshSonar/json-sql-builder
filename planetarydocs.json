{
    "settings": {
        "static": {
            "title": "json-sql-builder",
            "titleSmall": "json-sql-builder",
            "footer": {
                "first": "Generated by planetarydev:planetdocs",
                "second": "Licence MIT",
                "thanks": ""
            },
            "theme": "blue-grey"
        },
        "highlightjs": {
            "theme": "github"
        },
        "git": "http://github.com/planetarydev/json-sql-builder/blob/master/",
        "README.md": true,
        "includes": [
            {   "displayName": "Licence",
                "depth": 1,
                "filename": "LICENSE.md",
                "template": "includes.pug",
                "saveAs": "licence.html",
                "markdown": true
            },
			{   "displayName": "Roadmap",
                "depth": 1,
                "filename": "Roadmap.md",
                "template": "includes.pug",
                "saveAs": "roadmap.html",
                "markdown": true
            }
        ],
        "CRLF": "\n",
        "defaults": {
            "category": "apis"
        }
    },
    "ignoreFiles": {
        ".git": true,
        ".npm":true,
        "docs":true,
        "node_modules":true,
		"test":true
    },
    "sourceExtensions": {
        "js": true
    },
    "categories": {
        "$DEFAULT": "Api",
        "istemplate": {
            "category": "Templates"
        },
        "ismethod": {
            "category": "Methods"
        },
        "ishelper": {
            "category": "Helpers"
        },
        "isvalidator": {
            "category": "Validators"
        },
        "isquerying": {
            "category": "Querying"
        }
    },
    "tags": {
        "isvalidator":{
            "type": "boolean",
            "default": true
        },
        "istemplate": {
            "type": "boolean",
            "default": true
        },
        "ismethod": {
            "type": "boolean",
            "default": true
        },
        "ishelper": {
            "type": "boolean",
            "default": true
        },
        "isquerying": {
            "type": "boolean",
            "default": true
        }
    }
}
